<caldavtest>
	<description>Test GET on collections to ensure proper directory listing, calendar aggregation behavior</description>

	<require-feature>
		<feature>caldav</feature>
		<feature>PATCH Method</feature>
		<feature>patch-calendar-object</feature>
	</require-feature>

	<start />

	<test-suite name='OPTION on resource'>
		<test name='1'>
			<description>PUT event</description>
			<request>
				<method>PUT</method>
				<ruri>$calendarpath1:/1.ics</ruri>
				<data>
					<content-type>text/calendar; charset=utf-8</content-type>
					<filepath>Resource/CalDAV/patch/property-create/1.ics</filepath>
				</data>
				<verify>
					<callback>statusCode</callback>
				</verify>
			</request>
			<request>
				<method>GET</method>
				<ruri>$calendarpath1:/1.ics</ruri>
				<verify>
					<callback>calendarDataMatch</callback>
					<arg>
						<name>filepath</name>
						<value>Resource/CalDAV/patch/property-create/1.ics</value>
					</arg>
				</verify>
			</request>
		</test>
		<test name='2'>
			<description>OPTIONS event</description>
			<request>
				<method>OPTIONS</method>
				<ruri>$calendarpath1:/1.ics</ruri>
				<verify>
					<callback>statusCode</callback>
				</verify>
				<verify>
					<callback>header</callback>
					<arg>
						<name>header</name>
						<value>Allow$.*PATCH.*</value>
						<value>Accept-Patch$application/calendar-patch</value>
					</arg>
				</verify>
			</request>
		</test>
		<test name='-1'>
			<description>DELETE event</description>
			<request>
				<method>DELETE</method>
				<ruri>$calendarpath1:/1.ics</ruri>
				<verify>
					<callback>statusCode</callback>
				</verify>
			</request>
		</test>
	</test-suite>

